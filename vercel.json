{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "node_modules/swagger-ui-dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/index.js"
    },
    {
      "src": "/swagger.json",
      "dest": "/index.js"
    },
    {
      "src": "/swagger-ui-dist/(.*)",
      "dest": "/node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
